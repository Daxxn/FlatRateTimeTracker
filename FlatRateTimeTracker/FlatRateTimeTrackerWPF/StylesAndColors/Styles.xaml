<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style TargetType="Border" x:Key="TT_Job_Border">
		<Setter Property="BorderBrush" Value="{StaticResource TT_JobBorderBrush}"/>
		<Setter Property="BorderThickness" Value="1"/>
		<Setter Property="Background" Value="{StaticResource TT_JobBackgroundColor}"/>
	</Style>
</ResourceDictionary>